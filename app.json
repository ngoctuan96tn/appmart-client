{
  "expo": {
    "name": "MiMart",
    "slug": "MiMart",
    "version": "2.2.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon_ios.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/MiMartLogoGradientBlue.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vp.mimart",
      "infoPlist": {
        "NSCameraUsageDescription": "Mi Mart uses your camera roll to allow you to select a custom avatar!"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/MiMartLogoGradientApp.png",
        "backgroundColor": "#ffffff"
      },
      "versionCode": 2,
      "package": "com.vp.mimart"
    },
    "web": {
      "favicon": "./assets/images/MiMartLogoGradientApp.png"
    }
  }
}
